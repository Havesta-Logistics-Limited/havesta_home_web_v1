{
  "name": "harvesta",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --port 3001",
    "build": "vite build",
    "analyze": "vite build --mode analyze",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "assets:svg:sizes": "node -e \"const fs=require('fs'); const path=require('path'); const dir='src/assets/images'; const files=fs.readdirSync(dir).filter(f=>f.endsWith('.svg')).map(f=>path.join(dir,f)); for (const f of files){ const s=fs.statSync(f).size; console.log(String(s).padStart(10,' '), f); }\"",
    "assets:svg:optimize": "npx svgo -q --config=svgo.config.js src/assets/images/*.svg"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.19",
    "@mui/joy": "^5.0.0-beta.36",
    "@mui/material": "^5.15.19",
    "axios": "^1.7.5",
    "browser-image-compression": "^2.0.2",
    "formik": "^2.4.6",
    "framer-motion": "^11.2.10",
    "lucide-react": "^0.563.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.10.3",
    "react-router-dom": "^6.23.1",
    "react-router-hash-link": "^2.4.3",
    "react-toastify": "^10.0.5",
    "react-use-intercom": "^5.4.1",
    "swiper": "^11.1.4"
  },
  "devDependencies": {
    "@resvg/resvg-js": "^2.6.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "rollup": "^4.21.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "terser": "^5.46.0",
    "vite": "^5.2.0"
  }
}
